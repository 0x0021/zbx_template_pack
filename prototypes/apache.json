[
	{
		"_id": "apache.status",
		"key": "apache.get_status",
		"name": "Get status",
		"delay": "1m",
		"_zbx_ver": "3.4",
		"history": "1h",
		"trends": "0",
		"_group": "Zabbix_raw_items",
		"_resource": "Apache",
		"value_type": "TEXT",
		"description": "Getting data from a machine-readable version of the Apache status page",
		"triggers": [
			{
				"_prototype": "trigger.http_get",
				"expression": "{TEMPLATE_NAME:METRIC.nodata(30m)}=1",
				"name": "Failed to fetch status page (or no data for 30m)",
				"url": "https://httpd.apache.org/docs/current/mod/mod_status.html",
				"description": "Zabbix has not received data for items for the last 30 minutes.",
				"_depends_on": [
					"trigger.status",
					"trigger.proc.not_running"
				]
			}
		]
	},
	{
		"_id": "apache.bytes.rate",
		"key": "apache.bytes.rate",
		"name": "Bytes per second",
		"description": "Calculated as change rate for 'Total bytes' stat.\nBytesPerSec is not used, as it counts average since last Apache server start.",
		"type": "DEPENDENT",
		"masterItem": "apache.status",
		"_group": "Apache",
		"_resource": "Apache",
		"history": "7d",
		"units": "Bps",
		"value_type": "FLOAT",
		"description": "",
		"preprocessing": [
			{
				"type": "JSONPATH",
				"params": "$[\"Total kBytes\"]"
			},
			{
				"type": "MULTIPLIER",
				"params": "1024"
			},
			{
				"type": "CHANGE_PER_SECOND"
			}
		]
	},
