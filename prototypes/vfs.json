[
	//units not needed anymore
	{
		"name": "Storage units",
		"_id": "vfs.fs.units",
		"_group": "Zabbix_raw_items",
		"history": "1h",
		"trends": "0d"
	},
	{
		"name": "Used storage in units",
		"_id": "vfs.fs.units.used",
		"_group": "Zabbix_raw_items",
		"description": "Used storage in units",
		"units": "units",
		"history": "1h",
		"trends": "0d"
	},
	{
		"name": "Total space in units",
		"_id": "vfs.fs.units.total",
		"_group": "Zabbix_raw_items",
		"description": "Total space in units",
		"units": "units",
		"history": "1h",
		"trends": "0d"
	},
	{
		"_id": "vfs.fs.used",
		"name": "Used space",
		"description": "Used storage in Bytes",
		"_group": "Storage",
		"units": "B"
	},
	{
		"_id": "vfs.fs.total",
		"name": "Total space",
		"_group": "Storage",
		"units": "B",
		"description": "Total space in Bytes",
		"graphs": [
			{
				"name": "Disk space usage",
				"graphType": "PIE",
				"width": 600,
				"height": 340,
				"show_3d": 1,
				"graphItems": [
					{
						"name": "vfs.fs.total",
						"type": "GRAPH_SUM",
						"calc_fnc": "LAST",
						"color": "969696"
					},
					{
						"name": "vfs.fs.used",
						"type": "SIMPLE",
						"calc_fnc": "LAST",
						"color": "C80000"
					}
				]
			}
		]
	},
	{
		"_id": "vfs.fs.free",
		"name": "Free space",
		"_group": "Storage",
		"units": "B"
	},
	{
		"_id": "vfs.fs.pused",
		"name": "Space utilization",
		"_group": "Storage",
		"value_type": "FLOAT",
		"units": "%",
		"description": "Space utilization in % for __RESOURCE__",
		"triggers": [
			{
				"_prototype": "trigger.vfs.fs.crit_with_timeleft"
			},
			{
				"_prototype": "trigger.vfs.fs.warn_with_timeleft"
			}
		]
	},
	{
		"_id": "vfs.fs.inode.pfree",
		"name": "Free inodes in %",
		"_group": "Storage",
		"value_type": "FLOAT",
		"units": "%",
		"triggers": [
			{
				"_id": "trigger.inode.pfree.crit",
				"expression": "{TEMPLATE_NAME:METRIC.min(5m)}<{$VFS.FS.INODE.PFREE.MIN.CRIT:\"__RESOURCE__\"}",
				"name": "Running out of free inodes (free < {$VFS.FS.INODE.PFREE.MIN.CRIT:\"__RESOURCE__\"}%)",
				"priority": "AVERAGE",
				"description": "It may become impossible to write to disk if there are no index nodes left.\nAs symptoms, 'No space left on device' or 'Disk is full' errors may be seen even though free space is available.",
				"translations": {
					"RU": {
						"name": "Мало inodes на диске! (< {$VFS.FS.INODE.PFREE.MIN.CRIT:\"__RESOURCE__\"}%)"
					}
				}
			},
			{
				"_id": "trigger.inode.pfree.warn",
				"expression": "{TEMPLATE_NAME:METRIC.min(5m)}<{$VFS.FS.INODE.PFREE.MIN.WARN:\"__RESOURCE__\"}",
				"name": "Running out of free inodes (free < {$VFS.FS.INODE.PFREE.MIN.WARN:\"__RESOURCE__\"}%)",
				"priority": "WARNING",
				"description": "It may become impossible to write to disk if there are no index nodes left.\nAs symptoms, 'No space left on device' or 'Disk is full' errors may be seen even though free space is available.",
				"dependsOn": [
					"trigger.inode.pfree.crit"
				],
				"translations": {
					"RU": {
						"name": "Мало inodes на диске! (< {$VFS.FS.INODE.PFREE.MIN.WARN:\"__RESOURCE__\"}%)"
					}
				}
			}
		]
	},
	//PERFORMANCE
	{
		"_id": "vfs.dev.read.rate",
		"name": "Disk read rate",
		"_group": "Storage",
		"value_type": "FLOAT",
		"graphs": [
			{
				"name": "Disk read/write rates",
				"graphtype": "NORMAL",
				"graph_items": [
					{
						"drawtype": "SINGLE_LINE",
						"name": "vfs.dev.read.rate"
					},
					{
						"drawtype": "GRADIENT_LINE",
						"name": "vfs.dev.write.rate"
					}
				]
			}
		]
	},
	{
		"_id": "vfs.dev.write.rate",
		"name": "Disk write rate",
		"_group": "Storage",
		"value_type": "FLOAT"
	},
	{
		"_id": "vfs.dev.read.time.rate",
		"name": "Disk read time (rate)",
		"_group": "Storage",
		"value_type": "FLOAT"
	},
	{
		"_id": "vfs.dev.write.time.rate",
		"name": "Disk write time (rate)",
		"_group": "Storage",
		"value_type": "FLOAT"
	},
	{
		"_id": "vfs.dev.read.await",
		"name": "Disk read request avg waiting time (r_await)",
		"_group": "Storage",
		"units": "!ms",
		"update": "1m",
		"value_type": "FLOAT",
		"description": "The average time for read requests issued to the device to be served. This includes the time spent by the requests in queue and the time spent servicing them.",
		"triggers": [
			{
				"name": "Disk read request response are too high (read > {$VFS.DEV.READ.AWAIT.WARN:\"__RESOURCE__\"} ms for 5m or write > {$VFS.DEV.WRITE.AWAIT.WARN:\"__RESOURCE__\"} ms for 5m)",
				"expression": "{TEMPLATE_NAME:METRIC.min(5m)} > {$VFS.DEV.READ.AWAIT.WARN:\"__RESOURCE__\"} or {TEMPLATE_NAME:METRIC.min(5m)} > {$VFS.DEV.WRITE.AWAIT.WARN:\"__RESOURCE__\"}",
				"description": "This trigger might indicate disk __RESOURCE__ saturation.",
				"manual_close": "YES",
				"priority": "WARNING"
			}
		],
		"graphs": [
			{
				"name": "Disk average waiting time",
				"graphtype": "NORMAL",
				"graph_items": [
					{
						"drawtype": "SINGLE_LINE",
						"name": "vfs.dev.read.await"
					},
					{
						"drawtype": "GRADIENT_LINE",
						"name": "vfs.dev.write.await"
					}
				]
			}
		]
	},
	{
		"_id": "vfs.dev.write.await",
		"name": "Disk write request avg waiting time (w_await)",
		"_group": "Storage",
		"units": "!ms",
		"update": "1m",
		"value_type": "FLOAT",
		"description": "The average time for write requests issued to the device to be served. This includes the time spent by the requests in queue and the time spent servicing them."
	},
	{
		"_id": "vfs.dev.queue_size",
		"name": "Disk average queue size (avgqu-sz)",
		"_group": "Storage",
		"update": "1m",
		"value_type": "FLOAT"
	},
	{
		"_id": "vfs.dev.util",
		"name": "Disk utilization",
		"_group": "Storage",
		"update": "1m",
		"value_type": "FLOAT",
		"units": "%"
	}
]