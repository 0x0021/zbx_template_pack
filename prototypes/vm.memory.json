[
	{
		"id": "vm.memory.used",
		"name": "Used memory",
		"description": "Used memory in Bytes",
		"group": "Memory",
		"units": "B",
		"delay": "3m",
		"history": "30d",
		"trends": "365d"
	},
	{
		"id": "vm.memory.free",
		"name": "Free memory",
		"group": "Memory",
		"units": "B",
		"delay": "3m",
		"history": "30d",
		"trends": "365d"
	},
	{
		"id": "vm.memory.available",
		"name": "Available memory",
		"group": "Memory",
		"units": "B",
		"delay": "3m",
		"history": "30d",
		"trends": "365d",
		"description": "Available memory, in Linux, available = free + buffers + cache. On other platforms calculation may vary. See also: https://www.zabbix.com/documentation/current/manual/appendix/items/vm.memory.size_params",
		"triggers": [
			{
				"name": "Lack of available memory ({ITEM.VALUE1} of {ITEM.VALUE2})",
				"expression": "{TEMPLATE_NAME:METRIC.last(0)}<{$MEMORY_AVAILABLE_MIN} and {TEMPLATE_NAME:__vm.memory.total__.last(0)}>0",
				"priority":3
			}
		],
		"graphs": [
			{
				"name": "Memory usage",
				"graphType": "NORMAL",
				"yaxismin": 0,
				"ymin_type": "FIXED",
				"graphItems": [
					{
						"name": "vm.memory.total",
						"drawtype": "BOLD_LINE"
					},
					{
						"name": "vm.memory.available",
						"drawtype": "GRADIENT_LINE"
					}
				]
			}
		]
	},
	{
		"id": "vm.memory.total",
		"name": "Total memory",
		"group": "Memory",
		"units": "B",
		"delay": "3m",
		"history": "30d",
		"trends": "365d",
		"description": "Total memory in Bytes"
	},
	{
		"id": "vm.memory.pused",
		"name": "Memory utilization",
		"group": "Memory",
		"valueType": "FLOAT",
		"units": "%",
		"description": "Memory utilization in %",
		"delay": "3m",
		"history": "30d",
		"trends": "365d",
		"triggers": [
			{
				"prototype": "trigger.memory.high"
			}
		],
		"graphs": [
			{
				"name": "Memory utilization",
				"yaxismin": 0,
				"yaxismax": 100,
				"graphItems": [
					{
						"drawtype": "GRADIENT_LINE",
						"name": "vm.memory.pused"
					}
				]
			}
		]
	},
	{
		"id":"vm.memory.units",
		"name": "Memory units",
		"group": "Internal_items",
		"delay": "3m",
		"trends": "0",
		"history": "1w"
	},
	{
		"id":"vm.memory.units.used",
		"name": "Used memory in units",
		"group": "Internal_items",
		"units":"units",
		"delay": "3m",
		"trends": "0",
		"history": "1w",
		"description": "Used memory in units"
	},
	{
		"id":"vm.memory.units.total",
		"name": "Total memory in units",
		"group": "Internal_items",
		"units":"units",
		"delay": "3m",
		"trends": "0",
		"history": "1w",
		"description": "Total memory in units"
	},
	{
		"id": "system.swap.total",
		"name": "Total swap space",
		"group": "Memory",
		"units":"B",
		"delay": "5m"
	},
	{
		"id": "system.swap.free",
		"name": "Free swap space",
		"group": "Memory",
		"units":"B",
		"delay": "5m"
	},
	{
		"id": "system.swap.pfree",
		"name": "Free swap space in %",
		"group": "Memory",
		"units":"%",
		"valueType": "FLOAT",
		"delay": "5m",
		"triggers": [
			{
				"expression": "{TEMPLATE_NAME:METRIC.last()}<{$SWAP_PFREE_WARN} and {TEMPLATE_NAME:__system.swap.total__.last()}>0",
				"name": "High swap space usage (free: {ITEM.VALUE1}, total: {ITEM.VALUE2})",
				"priority": 2,
				"description": "This trigger is ignored, if there is no swap configured"
			}
		]
	}
]