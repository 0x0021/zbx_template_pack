[
	{
		"_id": "nginx.get_stub_status",
		"name": "Get stub status page",
		"delay": "1m",
		"_zbx_ver": "3.4",
		"history": "1h",
		"trends": "0",
		"_group": "Zabbix_raw_items",
		"_resource": "Nginx",
		"value_type": "TEXT",
		"description": "The following status information is provided:\nActive connections - the current number of active client connections including Waiting connections.\nAccepts - the total number of accepted client connections.\nHandled - the total number of handled connections. Generally, the parameter value is the same as accepts unless some resource limits have been reached (for example, the worker_connections limit).\nRequests - the total number of client requests.\nReading - the current number of connections where nginx is reading the request header.\nWriting - the current number of connections where nginx is writing the response back to the client.\nWaiting - the current number of idle client connections waiting for a request.",
		"triggers": [
			{
				"_prototype": "trigger.http_get",
				"name": "Failed to fetch stub status page (or no data for 30m)",
				"url": "https://nginx.ru/en/docs/http/ngx_http_stub_status_module.html",
				"description": "Zabbix has not received data for items for the last 30 minutes.",
				"_depends_on": [
					"trigger.perf",
					"trigger.proc.not_running",
					"trigger.status"
				]
			}
			/* Parsing:
		Active connections: 1 
		server accepts handled requests
 		3 3 3 
		Reading: 0 Writing: 1 Waiting: 0
		*/
		]
	},
	{
		"_id": "nginx.connections.accepted.rate",
		"key": "nginx.connections.accepted.rate",
		"type": "DEPENDENT",
		"value_type": "FLOAT",
		"name": "Connections accepted per second",
		"history": "7d",
		"_group": "Nginx",
		"_resource": "Nginx",
		"masterItem": "nginx.get_stub_status",
		"description": "The total number of accepted client connections.",
		"preprocessing": [
			{
				"type": "REGEX",
				"params": "server accepts handled requests\\s+([0-9]+) ([0-9]+) ([0-9]+)\r\n\\1"
			},
			{
				"type": "CHANGE_PER_SECOND"
			}
		]
	},
	{
		"_id": "nginx.connections.dropped.rate",
		"key": "nginx.connections.dropped.rate",
		"type": "DEPENDENT",
		"value_type": "FLOAT",
		"name": "Connections dropped per second",
		"history": "7d",
		"_group": "Nginx",
		"_resource": "Nginx",
		"masterItem": "nginx.get_stub_status",
		"description": "The total number of dropped client connections.",
		"preprocessing": [
			{
				"type": "JAVASCRIPT",
				"params": "var a = value.match(/server accepts handled requests\\s+([0-9]+) ([0-9]+) ([0-9]+)/)\nif (a) {\n    return a[1]-a[2]\n}"
			},
			{
				"type": "CHANGE_PER_SECOND"
			}
		]
	},
	{
		"_id": "nginx.connections.active",
		"name": "Connections active",
		"key": "nginx.connections.active",
		"type": "DEPENDENT",
		"masterItem": "nginx.get_stub_status",
		"history": "7d",
		"_group": "Nginx",
		"_resource": "Nginx",
		"description": "The current number of active client connections including Waiting connections.",
		"preprocessing": [
			{
				"type": "REGEX",
				"params": "Active connections: ([0-9]+)\r\n\\1"
			}
		],
		"graphs": [
			{
				"_id": "nginx.connections",
