					{
						"name": "nginx.requests.total.rate",
						"drawtype": "GRADIENT_LINE"
					},
					{
						"name": "nginx.connections.dropped.rate",
						"drawtype": "GRADIENT_LINE"
					}
				]
			}
		]
	},
