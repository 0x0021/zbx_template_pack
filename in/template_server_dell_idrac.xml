<?xml version="1.0" encoding="UTF-8"?>
<root xmlns="http://www.example.org/zbx_template_new/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.example.org/zbx_template_new/ ../../src/main/resources/templates/metrics.xsd">
    <template>
        <classes>
            <class>Fault</class>
            <class>Inventory</class>
            <class>Server</class>
            <class>SNMP Device</class>
            <class>SNMPv2</class>
        </classes>
        <name>Template Server Dell iDRAC</name>
        <description>Template Server iDRAC</description>
        <documentation>
            <overview>for Dell servers with iDRAC controllers
                http://www.dell.com/support/manuals/us/en/19/dell-openmanage-server-administrator-v8.3/snmp_idrac8/idrac-mib?guid=guid-e686536d-bc8e-4e09-8e8b-de8eb052efee
                Supported systems: http://www.dell.com/support/manuals/us/en/04/dell-openmanage-server-administrator-v8.3/snmp_idrac8/supported-systems?guid=guid-f72b75ba-e686-4e8a-b8c5-ca11c7c21381</overview>
            <known_devices>
                <device>
                    <name>iDRAC7, PowerEdge R620</name>
                    <oid>1.3.6.1.4.1.674.10892.5</oid>
                </device>
                <device>
                    <name>iDRAC8, PowerEdge R730xd</name>
                    <oid>1.3.6.1.4.1.674.10892.5</oid>
                </device>
                <device>
                    <name>iDRAC8, PowerEdge R720</name>
                    <oid>1.3.6.1.4.1.674.10892.5</oid>
                </device>
            </known_devices>
        </documentation>
        <discoveryRules>
            <discoveryRule>
                <name>Temperature CPU Discovery</name>
                <snmp_oid>discovery[{#SNMPVALUE},1.3.6.1.4.1.674.10892.5.4.700.20.1.1,{#SENSOR_LOCALE},1.3.6.1.4.1.674.10892.5.4.700.20.1.8]</snmp_oid>
                <key>temp.cpu.discovery</key>
                <description>Scanning table of Temperature Probe Table IDRAC-MIB-SMIv2::temperatureProbeTable</description>
                <filter>
                    <evaltype>0</evaltype>
                    <formula/>
                    <conditions>
                        <condition>
                            <macro>{#SENSOR_LOCALE}</macro>
                            <value>.*CPU.*</value><!-- cpu -->
                            <operator>8</operator>
                            <formulaid>A</formulaid>
                        </condition>
                    </conditions>
                </filter>
            </discoveryRule>

            <discoveryRule>
                <name>Temperature Ambient Discovery</name>
                <snmp_oid>discovery[{#SNMPVALUE},1.3.6.1.4.1.674.10892.5.4.700.20.1.1,{#SENSOR_LOCALE},1.3.6.1.4.1.674.10892.5.4.700.20.1.8]</snmp_oid>
                <key>temp.ambient.discovery</key>
                <description>Scanning table of Temperature Probe Table IDRAC-MIB-SMIv2::temperatureProbeTable</description>
                <filter>
                    <evaltype>0</evaltype>
                    <formula/>
                    <conditions>
                        <condition>
                            <macro>{#SENSOR_LOCALE}</macro>
                            <value>.*Inlet Temp.*</value><!-- ambient, inlet temp -->
                            <operator>8</operator>
                            <formulaid>A</formulaid>
                        </condition>
                    </conditions>
                </filter>
            </discoveryRule>


            <discoveryRule>
                <name>PSU Discovery</name>
                <snmp_oid>discovery[{#SNMPVALUE},1.3.6.1.4.1.674.10892.5.4.600.12.1.5,{#PSU_DESCR},1.3.6.1.4.1.674.10892.5.4.600.12.1.8]</snmp_oid>
                <key>psu.discovery</key>
                <description>IDRAC-MIB-SMIv2::powerSupplyTable</description>
            </discoveryRule>

            <discoveryRule>
                <name>FAN Discovery</name>
                <snmp_oid>discovery[{#SNMPVALUE},1.3.6.1.4.1.674.10892.5.4.700.12.1.5,{#FAN_DESCR},1.3.6.1.4.1.674.10892.5.4.700.12.1.8,{#TYPE},1.3.6.1.4.1.674.10892.5.4.700.12.1.7]</snmp_oid>
                <key>fan.discovery</key>
                <description>IDRAC-MIB-SMIv2::coolingDeviceTable</description>
                <filter>
                    <evaltype>0</evaltype>
                    <formula/>
                    <conditions>
                        <condition>
                            <macro>{#TYPE}</macro>
                            <value>3</value>  <!-- fan -->
                            <operator>8</operator>
                            <formulaid>A</formulaid>
                        </condition>
                    </conditions>
                </filter>
            </discoveryRule>

            <discoveryRule>
                <name>Physical Disk Discovery</name>
                <snmp_oid>discovery[{#SNMPVALUE},1.3.6.1.4.1.674.10892.5.5.1.20.130.4.1.1,{#DISK_NAME},1.3.6.1.4.1.674.10892.5.5.1.20.130.4.1.2]</snmp_oid>
                <key>physicaldisk.discovery</key>
                <description>IDRAC-MIB-SMIv2::physicalDiskTable</description>
            </discoveryRule>

            <discoveryRule>
                <name>Virtual Disk Discovery</name>
                <snmp_oid>discovery[{#SNMPVALUE},1.3.6.1.4.1.674.10892.5.5.1.20.140.1.1.1,{#DISK_NAME},1.3.6.1.4.1.674.10892.5.5.1.20.140.1.1.2]</snmp_oid>
                <key>virtualdisk.discovery</key>
                <description>IDRAC-MIB-SMIv2::virtualDiskTable</description>
            </discoveryRule>

            <discoveryRule>
                <name>Array Controller Discovery</name>
                <snmp_oid>discovery[{#SNMPVALUE},1.3.6.1.4.1.674.10892.5.5.1.20.130.1.1.38,{#CNTLR_NAME},1.3.6.1.4.1.674.10892.5.5.1.20.130.1.1.79]</snmp_oid>
                <key>physicaldisk.arr.discovery</key>
                <description>IDRAC-MIB-SMIv2::controllerTable</description>
            </discoveryRule>
            <discoveryRule>
                <name>Array Controller Cache Discovery</name>
                <snmp_oid>discovery[{#BATTERY_NUM},1.3.6.1.4.1.674.10892.5.5.1.20.130.15.1.1]</snmp_oid>
                <key>array.cache.discovery</key>
                <description>IDRAC-MIB-SMIv2::batteryTable</description>
            </discoveryRule>

        </discoveryRules>
        <metrics>
            <sensor.temp.value>
                <oid>1.3.6.1.4.1.674.10892.5.4.700.20.1.6.{#SNMPINDEX}</oid>
                <snmpObject>temperatureProbeReading.CPU.{#SNMPINDEX}</snmpObject>
                <mib>IDRAC-MIB-SMIv2</mib>
                <discoveryRule>Temperature CPU Discovery</discoveryRule>
                <alarmObject>{#SENSOR_LOCALE}</alarmObject>
                <alarmObjectType>CPU</alarmObjectType>
                <valueMap>IDRAC-MIB-SMIv2::StatusProbeEnum</valueMap>
                <vendorDescription>0700.0020.0001.0006 This attribute defines the reading for a temperature probe of type other than temperatureProbeTypeIsDiscrete.  When the value for temperatureProbeType is other than temperatureProbeTypeIsDiscrete,the value returned for this attribute is the temperature that the probeis reading in tenths of degrees Centigrade. When the value for temperatureProbeType is temperatureProbeTypeIsDiscrete, a value is not returned for this attribute.</vendorDescription>
                <preprocessing>
                    <step>
                        <type>multiplier</type>
                        <params>0.1</params>
                    </step>
                </preprocessing>
            </sensor.temp.value>

            <sensor.temp.status>
                <oid>1.3.6.1.4.1.674.10892.5.4.700.20.1.5.{#SNMPINDEX}</oid>
                <snmpObject>temperatureProbeStatus.CPU.{#SNMPINDEX}</snmpObject>
                <mib>IDRAC-MIB-SMIv2</mib>
                <discoveryRule>Temperature CPU Discovery</discoveryRule>
                <alarmObject>{#SENSOR_LOCALE}</alarmObject>
                <alarmObjectType>CPU</alarmObjectType>
                <vendorDescription>0700.0020.0001.0005 This attribute defines the probe status of the temperature probe.</vendorDescription>
                <valueMap>IDRAC-MIB-SMIv2::StatusProbeEnum</valueMap>
            </sensor.temp.status>

            <sensor.temp.value>
                <oid>1.3.6.1.4.1.674.10892.5.4.700.20.1.6.{#SNMPINDEX}</oid>
                <snmpObject>temperatureProbeReading.Ambient.{#SNMPINDEX}</snmpObject>
                <mib>IDRAC-MIB-SMIv2</mib>
                <discoveryRule>Temperature Ambient Discovery</discoveryRule>
                <alarmObject>{#SENSOR_LOCALE}</alarmObject>
                <alarmObjectType>Ambient</alarmObjectType>
                <valueMap>IDRAC-MIB-SMIv2::StatusProbeEnum</valueMap>
                <vendorDescription>0700.0020.0001.0006 This attribute defines the reading for a temperature probe of type other than temperatureProbeTypeIsDiscrete.  When the value for temperatureProbeType is other than temperatureProbeTypeIsDiscrete,the value returned for this attribute is the temperature that the probeis reading in tenths of degrees Centigrade. When the value for temperatureProbeType is temperatureProbeTypeIsDiscrete, a value is not returned for this attribute.</vendorDescription>
                <preprocessing>
                    <step>
                        <type>multiplier</type>
                        <params>0.1</params>
                    </step>
                </preprocessing>
            </sensor.temp.value>

            <sensor.temp.status>
                <oid>1.3.6.1.4.1.674.10892.5.4.700.20.1.5.{#SNMPINDEX}</oid>
                <snmpObject>temperatureProbeStatus.Ambient.{#SNMPINDEX}</snmpObject>
                <mib>IDRAC-MIB-SMIv2</mib>
                <discoveryRule>Temperature Ambient Discovery</discoveryRule>
                <alarmObject>{#SENSOR_LOCALE}</alarmObject>
                <alarmObjectType>Ambient</alarmObjectType>
                <vendorDescription>0700.0020.0001.0005 This attribute defines the probe status of the temperature probe.</vendorDescription>
                <valueMap>IDRAC-MIB-SMIv2::StatusProbeEnum</valueMap>
            </sensor.temp.status>


            <system.status>
                <oid>1.3.6.1.4.1.674.10892.5.2.1.0</oid>
                <snmpObject>globalSystemStatus.0</snmpObject>
                <mib>IDRAC-MIB-SMIv2</mib>
                <vendorDescription>This attribute defines the overall rollup status of all components in the system being monitored by the remote access card. Includes system, storage, IO devices, iDRAC, CPU, memory, etc.</vendorDescription>
                <valueMap>IDRAC-MIB-SMIv2::ObjectStatusEnum</valueMap>
            </system.status>

            <sensor.psu.status>
                <oid>1.3.6.1.4.1.674.10892.5.4.600.12.1.5.{#SNMPINDEX}</oid>
                <snmpObject>powerSupplyStatus.{#SNMPINDEX}</snmpObject>
                <mib>IDRAC-MIB-SMIv2</mib>
                <discoveryRule>PSU Discovery</discoveryRule>
                <vendorDescription>0600.0012.0001.0005 This attribute defines the status of the power supply.</vendorDescription>
                <valueMap>IDRAC-MIB-SMIv2::ObjectStatusEnum</valueMap>
                <alarmObject>{#PSU_DESCR}</alarmObject>
            </sensor.psu.status>

            <sensor.fan.status>
                <oid>1.3.6.1.4.1.674.10892.5.4.700.12.1.5.{#SNMPINDEX}</oid>
                <snmpObject>coolingDeviceStatus.{#SNMPINDEX}</snmpObject>
                <mib>IDRAC-MIB-SMIv2</mib>
                <discoveryRule>FAN Discovery</discoveryRule>
                <vendorDescription>0700.0012.0001.0005 This attribute defines the probe status of the cooling device.</vendorDescription>
                <valueMap>IDRAC-MIB-SMIv2::StatusProbeEnum</valueMap>
                <alarmObject>{#FAN_DESCR}</alarmObject>
            </sensor.fan.status>

            <sensor.fan.speed>
                <oid>1.3.6.1.4.1.674.10892.5.4.700.12.1.6.{#SNMPINDEX}</oid>
                <snmpObject>coolingDeviceReading.{#SNMPINDEX}</snmpObject>
                <mib>IDRAC-MIB-SMIv2</mib>
                <discoveryRule>FAN Discovery</discoveryRule>
                <vendorDescription>0700.0012.0001.0006 This attribute defines the reading for a cooling device
                    of subtype other than coolingDeviceSubTypeIsDiscrete.  When the value
                    for coolingDeviceSubType is other than coolingDeviceSubTypeIsDiscrete, the
                    value returned for this attribute is the speed in RPM or the OFF/ON value
                    of the cooling device.  When the value for coolingDeviceSubType is
                    coolingDeviceSubTypeIsDiscrete, a value is not returned for this attribute.</vendorDescription>
                <alarmObject>{#FAN_DESCR}</alarmObject>
            </sensor.fan.speed>


            <system.hw.diskarray.status>
                <oid>1.3.6.1.4.1.674.10892.5.5.1.20.130.1.1.38.{#SNMPINDEX}</oid>
                <snmpObject>controllerComponentStatus.{#SNMPINDEX}</snmpObject>
                <mib>IDRAC-MIB-SMIv2</mib>
                <discoveryRule>Array Controller Discovery</discoveryRule>
                <vendorDescription>The status of the controller itself without the propagation of any contained component status.
                    Possible values:
                    1: Other
                    2: Unknown
                    3: OK
                    4: Non-critical
                    5: Critical
                    6: Non-recoverable
                </vendorDescription>
                <alarmObject>{#CNTLR_NAME}</alarmObject>
                <valueMap>IDRAC-MIB-SMIv2::ObjectStatusEnum</valueMap>
            </system.hw.diskarray.status>

            <system.hw.diskarray.model>
                <oid>1.3.6.1.4.1.674.10892.5.5.1.20.130.1.1.2.{#SNMPINDEX}</oid>
                <snmpObject>controllerName.{#SNMPINDEX}</snmpObject>
                <mib>IDRAC-MIB-SMIv2</mib>
                <discoveryRule>Array Controller Discovery</discoveryRule>
                <vendorDescription>The controller's name as represented in Storage Management.</vendorDescription>
                <alarmObject>{#CNTLR_NAME}</alarmObject>
            </system.hw.diskarray.model>

            <system.hw.diskarray.cache.battery.status>
                <oid>1.3.6.1.4.1.674.10892.5.5.1.20.130.15.1.4.{#SNMPINDEX}</oid>
                <snmpObject>batteryState.{#SNMPINDEX}</snmpObject>
                <mib>IDRAC-MIB-SMIv2</mib>
                <discoveryRule>Array Controller Cache Discovery</discoveryRule>
                <vendorDescription>
                    Current state of battery.
                    Possible values:
                    1: The current state could not be determined.
                    2: The battery is operating normally.
                    3: The battery has failed and needs to be replaced.
                    4: The battery temperature is high or charge level is depleting.
                    5: The battery is missing or not detected.
                    6: The battery is undergoing the re-charge phase.
                    7: The battery voltage or charge level is below the threshold.
                </vendorDescription>
                <alarmObject>Battery {#BATTERY_NUM}</alarmObject>
                <valueMap>IDRAC-MIB-SMIv2::batteryState</valueMap>
            </system.hw.diskarray.cache.battery.status>

            <system.hw.physicaldisk.status>
                <oid>1.3.6.1.4.1.674.10892.5.5.1.20.130.4.1.24.{#SNMPINDEX}</oid>
                <snmpObject>physicalDiskComponentStatus.{#SNMPINDEX}</snmpObject>
                <mib>IDRAC-MIB-SMIv2</mib>
                <discoveryRule>Physical Disk Discovery</discoveryRule>
                <vendorDescription>The status of the physical disk itself without the propagation of any contained component status.
                    Possible values:
                    1: Other
                    2: Unknown
                    3: OK
                    4: Non-critical
                    5: Critical
                    6: Non-recoverable</vendorDescription>
                <alarmObject>{#DISK_NAME}</alarmObject>
                <valueMap>IDRAC-MIB-SMIv2::physicalDiskComponentStatus</valueMap>
            </system.hw.physicaldisk.status>

            <system.hw.physicaldisk.serialnumber>
                <oid>1.3.6.1.4.1.674.10892.5.5.1.20.130.4.1.7.{#SNMPINDEX}</oid>
                <snmpObject>physicalDiskSerialNo.{#SNMPINDEX}</snmpObject>
                <mib>IDRAC-MIB-SMIv2</mib>
                <discoveryRule>Physical Disk Discovery</discoveryRule>
                <vendorDescription>The physical disk's unique identification number from the manufacturer.</vendorDescription>
                <alarmObject>{#DISK_NAME}</alarmObject>
            </system.hw.physicaldisk.serialnumber>

            <system.hw.physicaldisk.smart_status>
                <oid>1.3.6.1.4.1.674.10892.5.5.1.20.130.4.1.31.{#SNMPINDEX}</oid>
                <snmpObject>physicalDiskSmartAlertIndication.{#SNMPINDEX}</snmpObject>
                <mib>IDRAC-MIB-SMIv2</mib>
                <discoveryRule>Physical Disk Discovery</discoveryRule>
                <vendorDescription>Indicates whether the physical disk has received a predictive failure alert.</vendorDescription>
                <alarmObject>{#DISK_NAME}</alarmObject>
                <valueMap>IDRAC-MIB-SMIv2::BooleanType</valueMap>
            </system.hw.physicaldisk.smart_status>

            <system.hw.physicaldisk.model>
                <oid>1.3.6.1.4.1.674.10892.5.5.1.20.130.4.1.6.{#SNMPINDEX}</oid>
                <snmpObject>physicalDiskProductID.{#SNMPINDEX}</snmpObject>
                <mib>IDRAC-MIB-SMIv2</mib>
                <discoveryRule>Physical Disk Discovery</discoveryRule>
                <vendorDescription>The model number of the physical disk.</vendorDescription>
                <alarmObject>{#DISK_NAME}</alarmObject>
            </system.hw.physicaldisk.model>

            <system.hw.physicaldisk.part_number>
                <oid>1.3.6.1.4.1.674.10892.5.5.1.20.130.4.1.27.{#SNMPINDEX}</oid>
                <snmpObject>physicalDiskPartNumber.{#SNMPINDEX}</snmpObject>
                <mib>IDRAC-MIB-SMIv2</mib>
                <discoveryRule>Physical Disk Discovery</discoveryRule>
                <vendorDescription>The part number of the disk.</vendorDescription>
                <alarmObject>{#DISK_NAME}</alarmObject>
            </system.hw.physicaldisk.part_number>

            <system.hw.physicaldisk.media_type>
                <oid>1.3.6.1.4.1.674.10892.5.5.1.20.130.4.1.35.{#SNMPINDEX}</oid>
                <snmpObject>physicalDiskMediaType.{#SNMPINDEX}</snmpObject>
                <mib>IDRAC-MIB-SMIv2</mib>
                <discoveryRule>Physical Disk Discovery</discoveryRule>
                <vendorDescription>The media type of the physical disk. Possible Values:
                    1: The media type could not be determined.
                    2: Hard Disk Drive (HDD).
                    3: Solid State Drive (SSD).</vendorDescription>
                <alarmObject>{#DISK_NAME}</alarmObject>
                <valueMap>IDRAC-MIB-SMIv2::physicalDiskMediaType</valueMap>
            </system.hw.physicaldisk.media_type>

            <system.hw.physicaldisk.size>
                <oid>1.3.6.1.4.1.674.10892.5.5.1.20.130.4.1.11.{#SNMPINDEX}</oid>
                <snmpObject>physicalDiskCapacityInMB.{#SNMPINDEX}</snmpObject>
                <mib>IDRAC-MIB-SMIv2</mib>
                <discoveryRule>Physical Disk Discovery</discoveryRule>
                <vendorDescription>The size of the physical disk in megabytes.</vendorDescription>
                <alarmObject>{#DISK_NAME}</alarmObject>
                <preprocessing>
                    <step>
                        <type>multiplier</type>
                        <params>1048576</params>
                    </step>
                </preprocessing>
            </system.hw.physicaldisk.size>

            <system.hw.virtualdisk.layout>
                <oid>1.3.6.1.4.1.674.10892.5.5.1.20.140.1.1.13.{#SNMPINDEX}</oid>
                <snmpObject>virtualDiskLayout.{#SNMPINDEX}</snmpObject>
                <mib>IDRAC-MIB-SMIv2</mib>
                <vendorDescription>The virtual disk's RAID type.
                    Possible values:
                    1: Not one of the following
                    2: RAID-0
                    3: RAID-1
                    4: RAID-5
                    5: RAID-6
                    6: RAID-10
                    7: RAID-50
                    8: RAID-60
                    9: Concatenated RAID 1
                    10: Concatenated RAID 5</vendorDescription>
                <discoveryRule>Virtual Disk Discovery</discoveryRule>
                <alarmObject>Disk {#SNMPVALUE}({#DISK_NAME})</alarmObject>
                <valueMap>IDRAC-MIB-SMIv2::virtualDiskLayout</valueMap>
            </system.hw.virtualdisk.layout>

            <system.hw.virtualdisk.state>
                <oid>1.3.6.1.4.1.674.10892.5.5.1.20.140.1.1.30.{#SNMPINDEX}</oid>
                <snmpObject>virtualDiskOperationalState.{#SNMPINDEX}</snmpObject>
                <mib>IDRAC-MIB-SMIv2</mib>
                <vendorDescription>The state of the virtual disk when there are progressive operations ongoing.
                    Possible values:
                    1: There is no active operation running.
                    2: The virtual disk configuration has changed. The physical disks included in the virtual disk are being modified to support the new configuration.
                    3: A Consistency Check (CC) is being performed on the virtual disk.
                    4: The virtual disk is being initialized.
                    5: BackGround Initialization (BGI) is being performed on the virtual disk.</vendorDescription>
                <discoveryRule>Virtual Disk Discovery</discoveryRule>
                <alarmObject>Disk {#SNMPVALUE}({#DISK_NAME})</alarmObject>
                <valueMap>IDRAC-MIB-SMIv2::virtualDiskOperationalState</valueMap>
            </system.hw.virtualdisk.state>

            <system.hw.virtualdisk.readpolicy>
                <oid>1.3.6.1.4.1.674.10892.5.5.1.20.140.1.1.11.{#SNMPINDEX}</oid>
                <snmpObject>virtualDiskReadPolicy.{#SNMPINDEX}</snmpObject>
                <mib>IDRAC-MIB-SMIv2</mib>
                <vendorDescription>The read policy used by the controller for read operations on this virtual disk.
                    Possible values:
                    1: No Read Ahead.
                    2: Read Ahead.
                    3: Adaptive Read Ahead.</vendorDescription>
                <discoveryRule>Virtual Disk Discovery</discoveryRule>
                <alarmObject>Disk {#SNMPVALUE}({#DISK_NAME})</alarmObject>
            </system.hw.virtualdisk.readpolicy>

            <system.hw.virtualdisk.writepolicy>
                <oid>1.3.6.1.4.1.674.10892.5.5.1.20.140.1.1.10.{#SNMPINDEX}</oid>
                <snmpObject>virtualDiskWritePolicy.{#SNMPINDEX}</snmpObject>
                <mib>IDRAC-MIB-SMIv2</mib>
                <vendorDescription>The write policy used by the controller for write operations on this virtual disk.
                    Possible values:
                    1: Write Through.
                    2: Write Back.
                    3: Force Write Back.</vendorDescription>
                <discoveryRule>Virtual Disk Discovery</discoveryRule>
                <alarmObject>Disk {#SNMPVALUE}({#DISK_NAME})</alarmObject>
                <valueMap>IDRAC-MIB-SMIv2::virtualDiskWritePolicy</valueMap>
            </system.hw.virtualdisk.writepolicy>

            <system.hw.virtualdisk.size>
                <oid>1.3.6.1.4.1.674.10892.5.5.1.20.140.1.1.6.{#SNMPINDEX}</oid>
                <snmpObject>virtualDiskSizeInMB.{#SNMPINDEX}</snmpObject>
                <mib>IDRAC-MIB-SMIv2</mib>
                <vendorDescription>The size of the virtual disk in megabytes.</vendorDescription>
                <discoveryRule>Virtual Disk Discovery</discoveryRule>
                <alarmObject>Disk {#SNMPVALUE}({#DISK_NAME})</alarmObject>
                <preprocessing>
                    <step>
                        <type>multiplier</type>
                        <params>1048576</params>
                    </step>
                </preprocessing>
            </system.hw.virtualdisk.size>


            <system.hw.virtualdisk.status>
                <oid>1.3.6.1.4.1.674.10892.5.5.1.20.140.1.1.4.{#SNMPINDEX}</oid>
                <snmpObject>virtualDiskState.{#SNMPINDEX}</snmpObject>
                <mib>IDRAC-MIB-SMIv2</mib>
                <vendorDescription>The current state of this virtual disk (which includes any member physical disks.)
                    Possible states:
                    1: The current state could not be determined.
                    2: The virtual disk is operating normally or optimally.
                    3: The virtual disk has encountered a failure. The data on disk is lost or is about to be lost.
                    4: The virtual disk encounterd a failure with one or all of the constituent redundant physical disks.
                    The data on the virtual disk might no longer be fault tolerant.</vendorDescription>
                <discoveryRule>Virtual Disk Discovery</discoveryRule>
                <alarmObject>Disk {#SNMPVALUE}({#DISK_NAME})</alarmObject>
                <valueMap>IDRAC-MIB-SMIv2::virtualDiskState</valueMap>
            </system.hw.virtualdisk.status>

            <!-- inventory -->
            <system.hw.model>
                <oid>1.3.6.1.4.1.674.10892.5.1.3.12.0</oid>
                <snmpObject>systemModelName.0</snmpObject>
                <mib>IDRAC-MIB-SMIv2</mib>
                <vendorDescription>This attribute defines the model name of the system.</vendorDescription>
            </system.hw.model>

            <system.sw.os>
                <oid>1.3.6.1.4.1.674.10892.5.1.3.6.0</oid>
                <snmpObject>systemOSName</snmpObject>
                <mib>IDRAC-MIB-SMIv2</mib>
                <vendorDescription>This attribute defines the name of the operating system that the hostis running.</vendorDescription>
